  {
    "name": "TFTP Server",
    "version": "1.0",
    "slug": "tftp-server",
    "description": "TFTP Server",
    "url": "https://github.com/akira0776/hassio-addons/tree/main/tftp-hpa",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "application",
    "boot": "auto",
    "map": ["share:rw", "media:rw"],
    "ingress": true,
    "options": {
      "tftproot": "/share/htdocs",
      "networks": "default",
    },
    "schema": {
      "tftproot": "str",
      "networks": "str",
    },
    "ports": {
      "69/udp": 69,
    },
    "ports_description": {
      "69/udp": "TFTP port",
    }
  }
