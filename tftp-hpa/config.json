  {
    "name": "TFTP Server",
    "version": "1.0",
    "slug": "tftp-server",
    "description": "TFTP Server",
    "url": "https://github.com/akira0776/hassio-addons/tree/main/tftp-hpa",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "system",
    "boot": "auto",
    "map": ["share:rw"],
    },
    "options": {
      "tftproot": "/share/tftp",
      "networks": "0.0.0.0/0"
    },
    "schema": {
      "tftproot": "str",
      "networks": "str"
    },
    "ports": {
      "69/udp": 69
    },
    "ports_description": {
      "69/udp": "TFTP port"
    }
  }
