  {
    "name": "tftp server",
    "version": "1.0",
    "slug": "tftp-server",
    "description": "TFTP Server",
    "url": "http://192.168.123.4/hassio-addons/tree/master/tftp-hpa",
    "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
    "startup": "application",
    "boot": "auto",
    "map": ["share:rw", "media:rw", "ssl"],
    "ingress": true,
    "panel_icon": "mdi:web-box",
    "options": {
      "document_root": "/share/htdocs",
      "default_conf": "default",
      "default_ssl_conf": "default",
      "website_name":null,
      "ssl": true,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem"
    },
    "schema": {
      "tftproot": "str",
      "networks": "str",
    },
    "ports": {
      "69/udp": 80,
    },
    "ports_description": {
      "69/urp": "Web interface http",
    }
  }
